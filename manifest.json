{
  "manifest_version": 2,
  "name": "TweetDeck Back Button",
  "version": "1.0",
  "description": "Enable back button on TweetDeck",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://tweetdeck.twitter.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
